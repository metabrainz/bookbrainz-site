{
	"compilerOptions": {
		"module": "ESNext",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"jsx": "preserve",
		"lib": ["ESNext", "DOM"],
		"noEmit": true,
		"skipLibCheck": true,
		"strict": false,
		"target": "ESNext"
	}
}
